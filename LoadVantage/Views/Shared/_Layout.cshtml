<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />
	<title>@ViewData["Title"] - LoadVantage</title>

	<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
	<link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
	

	<!-- Vendor CSS Files -->
	<link href="~/assets/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
	<link href="~/assets/vendor/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">
	<link href="~/assets/vendor/boxicons/css/boxicons.min.css" rel="stylesheet">
	<link href="~/assets/vendor/quill/quill.snow.css" rel="stylesheet">
	<link href="~/assets/vendor/quill/quill.bubble.css" rel="stylesheet">
	<link href="~/assets/vendor/remixicon/remixicon.css" rel="stylesheet">
	<link href="~/assets/vendor/simple-datatables/style.css" rel="stylesheet">

	<!-- Favicons -->
	<link href="~/assets/img/favicon.ico" rel="icon">
	<link href="~/assets/img/apple-touch-icon.png" rel="apple-touch-icon">

	<!-- Google Fonts -->
	<link href="https://fonts.gstatic.com" rel="preconnect">
	<link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Nunito:300,300i,400,400i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">
    

	<!-- Template Main CSS File -->
	<link href="/assets/css/style.css" rel="stylesheet">

</head>

<header id="header" class="header">

	<div class="">
		<div class="logo-container">
			<a class="logo d-flex align-items-left" sp-area="" asp-controller="Home" asp-action="Index">
				<div id="triangle" class="d-sm-none d-md-block">
					<svg id="Layer_1" data-name="Layer 1" version="1.1" viewBox="0 0 2000 2000">
						<polygon
							class="cls-1"
							points="928 781 1021 951 784.5 1371.97 1618 1371.97 1530.32 1544 509 1539 928 781"
						></polygon>
						<polygon
							class="cls-3"
							points="1618 1371.97 784.5 1371.97 874.93 1211 1346 1211 923.1 456 1110.06 456 1618 1371.97"
						></polygon>
						<g id="Layer_2" data-name="Layer 2">
							<polygon
								class="cls-2"
								points="418 1372.74 509 1539 928 781 1162.32 1211 1346 1211 923.1 456 418 1372.74"
							></polygon>
						</g>
					</svg>
				</div>
				<span class="d-none d-lg-block">LoadVantage</span>
			</a>
		</div>
	</div><!-- End Logo -->

	<div class="d-flex justify-content-center">
		<ul class="navbar-nav d-flex justify-content-between mb-2 mb-lg-0" id="links-area-layout">
			<li class="nav-item">
				<a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index"><button class="fancy-project-button">Home</button></a>
			</li>
			<li class="nav-item">
				<a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy"><button class="fancy-project-button">Privacy</button></a>
			</li>

			<partial name="_LoginPartial" />
		</ul>
		
	</div>




</header><!-- End Header -->
<body style="background: rgb(131,58,180);background: linear-gradient(90deg, rgba(131,58,180,1) 0%, rgba(29,255,253,1) 100%, rgba(252,176,69,1) 100%);">


<div class="container">
	<main role="main" class="pb-3">
		@RenderBody()
	</main>
</div>

<footer id="footer" class="footer">
	<div class="copyright">
			&copy; 2024 - LoadVantage - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
	</div>

</footer><!-- End Footer -->

<a href="#" class="back-to-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>

<!-- SignalR  -->
<script src="https://cdnjs.cloudflare.com/ajax/libs/microsoft-signalr/6.0.5/signalr.min.js"></script>

<script src="~/lib/jquery/dist/jquery.min.js"></script>

<!-- jQuery Validation -->
<script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
<script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>


<!-- Vendor JS Files -->
<script src="~/assets/vendor/apexcharts/apexcharts.min.js"></script>
<script src="~/assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
<script src="~/assets/vendor/chart.js/chart.umd.js"></script>
<script src="~/assets/vendor/echarts/echarts.min.js"></script>
<script src="~/assets/vendor/quill/quill.js"></script>
<script src="~/assets/vendor/simple-datatables/simple-datatables.js"></script>
<script src="~/assets/vendor/tinymce/tinymce.min.js"></script>
<script src="~/assets/vendor/php-email-form/validate.js"></script>


<!-- Template Main JS File -->
<script src="~/assets/js/main.js"></script>
<script src="~/js/site.js"></script>

<!-- Section for additional scripts -->
@await RenderSectionAsync("Scripts", required: false)


</body>

</html>
